type User {
  id: ID! @id
  name: String!
  writtenKudoses: [Kudos] @relation(name: "WrittenKudoses")
  ownKudoses: [Kudos] @relation(name: "OwnKudoses")
}

type Kudos {
  id: ID! @id
  title: String!
  author: User @relation(name: "WrittenKudoses")
  recipient: User @relation(name: "OwnKudoses")
}
